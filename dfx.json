{
    "version": 1,
    "canisters": {
        "api": {
            "type": "motoko",
            "main": "canisters/api/main.mo"
        },
        "auth": {
            "type": "motoko",
            "main": "canisters/auth/main.mo"
        },
        "transaction": {
            "type": "motoko",
            "main": "canisters/auth/main.mo"
        },
        "ai": {
            "type": "motoko",
            "main": "canisters/ai/main.mo"
        },
        "storage_canister": {
            "type": "motoko",
            "main": "canisters/storage/main.mo"
        },
        "frontend": {
            "type": "assets",
            "source": ["www/dist/"],
            "workspace": "frontend"
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral"
        },
        "ic": {
            "providers": ["https://mainnet.dfinity.network"],
            "type": "persistent"
        },
        "playground": {
            "providers": ["https://playground.dfinity.network"],
            "type": "ephemeral"
        }
    },
    "replica": {
        "subnet_type": "application"
    },
    "defaults": {
        "build": {
            "args": "",
            "packtool": "mops sources"
        }
    }
}